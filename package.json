{
  "name": "homebridge-states",
  "version": "1.0.2",
  "description": "An homebridge plugin to manage home automation states",
  "main": "dist/accessory.js",
  "scripts": {
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "clean": "rimraf ./dist",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build && npm version patch --m 'Release %s'",
    "postpublish": "npm run clean"
  },
  "author": "Romain DUBOC <romain@duboc.pro>",
  "engines": {
    "node": ">=10.17.0",
    "homebridge": ">=1.3.0"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "license": "ISC",
  "files": [
    "LICENSE",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dubocr/homebridge-states.git"
  },
  "bugs": {
    "url": "https://github.com/dubocr/homebridge-states.git/issues"
  },
  "devDependencies": {
    "@types/node": "^14.17.5",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.31.0",
    "homebridge": "^1.3.4",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/node-persist": "^3.1.2",
    "node-persist": "^3.1.0"
  }
}
